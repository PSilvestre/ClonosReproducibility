CONTEXT = [
  NETWORK = "YES",
  SET_HOSTNAME = "kubernetes-coordinator",
  SSH_PUBLIC_KEY = "<$ssh_key>",
  START_SCRIPT = "sudo mkfs.ext4 /dev/vdb && sudo mkdir -p /media/extra && sudo mount /dev/vdb /media/extra && sudo mkdir /media/extra/{0..200} && sudo chmod -R 777 /media/extra && sudo /home/ubuntu/create_cluster.sh <$cluster_name>",
  USERNAME = "ubuntu" ]
CPU = "<$coordinator_cpu>"
DISK = [
  IMAGE = "SIGMODReproClonos",
  IMAGE_UNAME = "delta",
	SIZE = 37000]
DISK = [
  FORMAT = "raw",
  SIZE = "<$coordinator_disk>",
  TYPE = "fs" ]
FEATURES = [
  ACPI = "yes",
  LOCALTIME = "no" ]
INPUT = [
  BUS = "usb",
  TYPE = "tablet" ]
LOGO = "images/logos/ubuntu.png"
MEMORY = "<$coordinator_memory>"
NIC = [
  NETWORK = "internet",
  NETWORK_UNAME = "oneadmin" ]
NIC = [
  NETWORK = "camparatia-tudelft.int",
  NETWORK_UNAME = "camparatia-asteriosk" ]
OS = [
  ARCH = "x86_64" ]
VCPU = "<$coordinator_cpu>"
